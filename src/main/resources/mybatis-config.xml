<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
	<typeAliases>
		<typeAlias type="kr.ac.kopo.strike.model.Member" alias="Member"/>
		<typeAlias type="kr.ac.kopo.strike.model.Game" alias="Game"/>
		<typeAlias type="kr.ac.kopo.strike.model.Challenger" alias="Challenger"/>
		<typeAlias type="kr.ac.kopo.strike.model.Clan" alias="Clan"/>
		<typeAlias type="kr.ac.kopo.strike.model.ClanMember" alias="ClanMember"/>
		<typeAlias type="kr.ac.kopo.strike.model.Franchisee" alias="Franchisee"/>
		<typeAlias type="kr.ac.kopo.strike.model.GameRecord" alias="GameRecord"/>
