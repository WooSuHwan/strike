		<mapper resource="mybatis/franchiseePage.xml"/>
		<mapper resource="mybatis/clanGame.xml"/>
		<mapper resource="mybatis/manager.xml"/>
		
		<mapper resource="mybatis/suggestfree.xml" />
      	<mapper resource="mybatis/suggestmaster.xml" />
      	<mapper resource="mybatis/reply.xml" />
	</mappers>
</configuration>