<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="franchiseeAdd">
	<select id="list" resultType="FranchiseeAdd">
		select *
		from franchisee_suggest
	</select>
	
	<insert id="add">
		INSERT INTO franchisee_suggest
		VALUES (
		FRANCHISEE_SUGGEST_CODE_SEQ.nextval,
		#{name}, 
		#{address}, 
		#{tel},
		sysdate
		)
	</insert>
	
	<select id="item" resultType="FranchiseeAdd">
		select *
		from franchisee_suggest
		where franchisee_suggest_code = #{franchisee_suggest_code}
	</select>
</mapper>